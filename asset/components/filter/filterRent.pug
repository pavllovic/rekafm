    div(class='ui-rangeBar ui-rangeBarSelect js-rangeBarInt')
      div(class='heading')
        span ставка, руб.:
        div(class='ui-select js-select')
          div(class='input js-input' role='combobox' tabindex='0' id='combobox'
            aria-activedescendant=''
            aria-autocomplete='none'
            aria-owns='listbox'
            aria-expanded='false'
            aria-haspopup='listbox'
            aria-labelledby='combobox-label')
            div(class='value') за все в месяц
            span(class='icon')
              svg(focusable='false' aria-hidden='true')
                use(href='#icon-16-dropdown-open')
          div(class='options js-options' role='listbox' id='listbox')
            label(class='option' role='option' id='option-price-1'  aria-selected='false')
              span(class='text') за все в месяц
              input(class='visually-hidden' type='checkbox' name='за все в месяц')
            label(class='option' role='option' id='option-price-2'  aria-selected='false') 
              span(class='text') за м2 в месяц
              input(class='visually-hidden' type='checkbox' name='за м2 в месяц')
            label(class='option' role='option' id='option-price-3'  aria-selected='false') 
              span(class='text') за все в год
              input(class='visually-hidden' type='checkbox' name='за все в год')
            label(class='option' role='option' id='option-price-4'  aria-selected='false') 
              span(class='text') за м2 в год
              input(class='visually-hidden' type='checkbox' name='за м2 в год')
            //- div(class='option' role='option' id='option-price-1'  aria-selected='false')
